{
	"name": "vndb-query",
	"version": "1.5.6",
	"description": "VNDB HTTPS API with custom query builder",
	"author": {
		"name": "Andrew Ferreira",
		"email": "andrew.shien2@gmail.com",
		"url": "https://github.com/ferreira-tb"
	},
	"homepage": "https://tb.dev.br/vndb-query",
	"repository": {
		"type": "git",
		"url": "https://github.com/ferreira-tb/vndb-query.git"
	},
	"bugs": {
		"url": "https://github.com/ferreira-tb/vndb-query/issues",
		"email": "andrew.shien2@gmail.com"
	},
	"license": "MIT",
	"keywords": [
		"vndb",
		"api",
		"http",
		"kana",
		"query builder",
		"visual novel"
	],
	"private": false,
	"main": "dist/index.js",
	"type": "module",
	"files": [
		"dist/**/*"
	],
	"types": "dist/src/index.d.ts",
	"lint-staged": {
		"*.{?(c|m)@(j|t)s,css,vue,md,json}": "prettier --write"
	},
	"scripts": {
		"build": "run-s rollup script:minify",
		"deploy": "run-s docs:build docs:typedoc",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:preview": "vitepress preview docs",
		"docs:typedoc": "npx typedoc --plugin typedoc-plugin-mdn-links",
		"prepare": "husky install",
		"prettier": "npx prettier . --write",
		"rollup": "rollup --config rollup.config.ts --configPlugin typescript={tsconfig:'tsconfig.rollup.json'}",
		"script:minify": "node scripts/minify.mjs",
		"script:publish": "node scripts/publish.mjs",
		"taze": "npx taze -w -f",
		"test": "vitest",
		"test:coverage": "vitest --coverage",
		"test:ui": "vitest --ui --watch --coverage.enabled=true",
		"test:watch": "vitest --watch"
	},
	"devDependencies": {
		"@octokit/core": "^5.0.1",
		"@rollup/plugin-typescript": "^11.1.5",
		"@unocss/preset-uno": "^0.57.4",
		"@vitest/coverage-istanbul": "^0.34.6",
		"@vitest/ui": "^0.34.6",
		"husky": "^8.0.3",
		"lint-staged": "^15.1.0",
		"npm-run-all2": "^6.1.1",
		"prettier": "^3.1.0",
		"rollup": "^4.4.1",
		"taze": "^0.12.0",
		"terser": "^5.24.0",
		"tslib": "^2.6.2",
		"typedoc": "^0.25.3",
		"typedoc-plugin-mdn-links": "^3.1.0",
		"typescript": "^5.2.2",
		"unocss": "^0.57.4",
		"vitepress": "^1.0.0-rc.25",
		"vitest": "^0.34.6",
		"vue": "^3.3.8",
		"zod": "^3.22.4"
	}
}
